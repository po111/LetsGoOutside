@model IEnumerable<EventServiceModel>


@{
    ViewBag.Title = "Моите събития";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (!Model.Any())
{
    <h2 class="text-center">Нямате публикувани събития.</h2>
}

<div class="row">
    @foreach (var eventt in Model)
    {
        <partial name="_EventPartial" model="@eventt" />
    }
</div>